{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public",
    "storyshots": "NODE_ENV=test jest --config ./jest.storyshots.config.js",
    "update-storyshots": "NODE_ENV=test jest --config ./jest.storyshots.config.js --updateSnapshot"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "ts-jest": "28.0.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-storyshots": "6.4.22",
    "@storybook/react": "6.4.22",
    "@testing-library/react": "13.2.0",
    "@types/jest": "27.5.0",
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/zxcvbn": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "autoprefixer": "10.4.7",
    "axios": "^0.27.2",
    "babel-jest": "28.1.0",
    "cssnano": "^5.1.7",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "formik": "^2.2.9",
    "husky": "^8.0.0",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-watch-typeahead": "1.1.0",
    "lint-staged": "12.4.1",
    "material-ui": "^0.20.2",
    "postcss": "^8.4.13",
    "postcss-nested": "5.0.6",
    "prettier": "2.6.2",
    "reg-keygen-git-hash-plugin": "0.11.1",
    "reg-notify-github-plugin": "0.11.1",
    "reg-notify-slack-plugin": "0.11.0",
    "reg-publish-s3-plugin": "0.11.0",
    "reg-suit": "0.11.1",
    "storycap": "3.1.7",
    "sweetalert2": "^11.4.13",
    "swr": "^1.3.0",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4",
    "yup": "^0.32.11"
  }
}
